{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "cube-generator",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/main.js"
    },
    "./style.css": "./dist/main.css"
  },
  "packageManager": "pnpm@10.6.1",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "format": "prettier --write ./src",
    "postinstall": "pnpm run build"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "dependencies": {
    "@elchininet/isometric": "^3.11.1",
    "@texel/color": "^1.1.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "prettier": "^3.5.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
